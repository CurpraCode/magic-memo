{
  "name": "magic-memo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git-cz",
    "postinstall": "husky install",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "lint-staged": {
    "./src/**": [
      "eslint src/ --fix",
      "prettier --write ."
    ]
